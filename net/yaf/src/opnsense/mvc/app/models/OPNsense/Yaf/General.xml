<model>
    <mount>//OPNsense/yaf/general</mount>
    <description>yaf configuration</description>
    <version>0.0.1</version>
    <items>
        <enabled type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </enabled>
        <interface type="InterfaceField">
            <Required>N</Required>
            <Multiple>N</Multiple>
        </interface>
        <yafhost type="TextField">
            <Required>Y</Required>
            <default>127.0.0.1</default>
        </yafhost>
        <yafport type="PortField">
            <Required>Y</Required>
            <default>18000</default>
        </yafport>
    </items>
</model>
